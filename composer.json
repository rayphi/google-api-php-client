{
    "name": "rayphi/google-apiclient",
    "type": "magento-module",
    "description": "Magento Client library for Google APIs",
    "keywords": ["google"],
    "homepage": "http://developers.google.com/api-client-library/php",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "suggest": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        },
        "map": [
            ["src/Google", "lib/Google"]
        ]
    }
}